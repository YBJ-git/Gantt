# 프로덕션 환경 변수 설정
# 이 파일을 .env.production으로 복사하여 사용하세요.

# API 기본 URL
REACT_APP_API_URL=https://api.workload-optimization.com

# 인증 관련 설정
REACT_APP_AUTH_DOMAIN=workload-optimization.auth0.com
REACT_APP_AUTH_CLIENT_ID=your_auth0_client_id

# 분석 및 로깅
REACT_APP_ANALYTICS_ID=UA-XXXXXXXXX-X
REACT_APP_LOG_LEVEL=error

# 기능 활성화
REACT_APP_ENABLE_ADVANCED_FEATURES=true
REACT_APP_ENABLE_BETA_FEATURES=false

# 캐싱 설정
REACT_APP_CACHE_DURATION=3600
